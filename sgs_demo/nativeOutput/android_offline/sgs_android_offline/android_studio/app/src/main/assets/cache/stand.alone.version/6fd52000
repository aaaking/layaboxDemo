var LangMgr = /** @class */ (function () {
    function LangMgr() {
        var obj = Laya.loader.getRes(LangMgr.mLang);
        Laya.Text.langPacks = obj;
    }
    Object.defineProperty(LangMgr, "instance", {
        get: function () {
            if (!this._instance) {
                this._instance = new LangMgr();
            }
            return this._instance;
        },
        enumerable: true,
        configurable: true
    });
    LangMgr.changeLang = function (langP) {
        LangMgr.mLang = langP;
    };
    LangMgr.LANG_ZH = "res/lang/zh.lang";
    LangMgr.LANG_ZH_TW = "res/lang/zh-TW.lang";
    LangMgr.LANG_EN = "res/lang/en.lang";
    LangMgr.mLang = LangMgr.LANG_ZH;
    LangMgr.back = "back";
    return LangMgr;
}());
//# sourceMappingURL=LangMgr.js.map