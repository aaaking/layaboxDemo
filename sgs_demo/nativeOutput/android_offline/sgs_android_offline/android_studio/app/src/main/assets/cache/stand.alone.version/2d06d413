/**
 * author: zhouzhihui
 */
var ParseData = /** @class */ (function () {
    function ParseData() {
    }
    /**
     * //解析图鉴数据
     * @param caller
     * @param callback
     */
    ParseData.parseWareHouseData = function (data) {
        //POST http://10.225.20.161:8118?{"jsonrpc":"2.0","method":"eth_call","params":[{"from":"0x2ee0faf9997edfd8bdc5d16939d91d1e3674a59f","to":"0xbbf97981efee7214874c304d7fed9788203cfa33","data":"0x3c3830342cb00000000000000000000000000000"},"latest"],"id":67}
        //"{"jsonrpc":"2.0","id":67,"result":""}"
        console.info(data);
        var info = JSON.parse(data);
        var result = info.result.substring(130);
        result = "0x" + result;
        var result1 = Utils.toAscii(result);
        console.log("result1: " + result1);
        var cards = result1.split(",");
        cards.pop();
        // cards = cards.concat(cards)//测试代码 50
        // cards = cards.concat(cards)//100
        // cards = cards.concat(cards)//200
        // cards = cards.concat(cards)//400
        // cards = cards.concat(cards)//800
        var resultData = [];
        for (var k in cards) {
            var v = cards[k];
            var arr = v.split(":");
            var id = parseInt(arr[0]);
            var cfg = GameConfig.getCfgHeroById(id);
            var count = arr[1];
            var isHave = false;
            var star = Math.floor(Math.random() * 8 + 3);
            cfg.star = star;
            if (CardPackageManager.instance.cards) {
                if (CardPackageManager.instance.judgeHaveById(id))
                    isHave = true;
            }
            var hasRendered = false;
            var originalY = -1;
            var t = {
                id: id, count: count, cfg: cfg, star: star, isHave: isHave, hasRendered: hasRendered, originalY: originalY
            };
            Constants.putCard(star, t);
        }
    };
    ParseData.parseExchangeData = function (data) {
        //POST http://10.225.20.161:8118?{"jsonrpc":"2.0","method":"eth_call","params":[{"from":"0xdcac9715a0012457310f0350dc984ff1dd3e51cb","to":"0xbbf97981efee7214874c304d7fed9788203cfa33","data":"0x3ded315b"},"latest"],"id":67}
        console.info(data);
        var info = JSON.parse(data);
        var result = info.result.substring(130);
        result = "0x" + result;
        var result1 = Utils.toAscii(result);
        console.log("result1: " + result1); //交易ID、cardID、price、self
        var cards = result1.split(";");
        cards.pop();
        var resultData = [];
        for (var k in cards) {
            var v = cards[k];
            var arr = v.split(",");
            var id = parseInt(arr[0]);
            var cfg = GameConfig.getCfgHeroById(parseInt(arr[1]));
            var price = arr[2];
            if (parseInt(price) <= 0)
                continue;
            var isself = arr[3] == "1";
            var t = {
                id: id, price: price, cfg: cfg, isself: isself
            };
            resultData.push(t);
        }
        console.info(resultData);
        return resultData;
    };
    ParseData.parseCardPackage = function (data) {
        //POST http://10.225.20.161:8118?{"jsonrpc":"2.0","method":"eth_call","params":[{"from":"0x24479b7f771d6d0d6d4003257ca1043661af7bd7","to":"0xbbf97981efee7214874c304d7fed9788203cfa33","data":"0x179a074f"},"latest"],"id":67}
        //{"jsonrpc":"2.0","id":67,"result":""}
        console.info(data);
        var info = JSON.parse(data);
        var result = info.result.substring(130);
        result = "0x" + result;
        var result1 = Utils.toAscii(result);
        console.log("result1: " + result1);
        var cards = result1.split(",");
        cards.pop();
        var resultData = [];
        for (var k in cards) {
            var v = cards[k];
            var arr = v.split(":");
            var id = parseInt(arr[0]);
            var cfg = GameConfig.getCfgHeroById(id);
            var count = arr[1];
            for (var cardIndex = 0; cardIndex < count; cardIndex++) {
                var t = {
                    id: id, count: count, cfg: cfg, isHave: Math.random() > 0.5
                };
                resultData.push(t);
            }
        }
        console.info(resultData); //[]
        return resultData;
    };
    return ParseData;
}());
